<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="org.zerock.test">

	<insert id="insertMember">
		insert into tbl_member2(uid,upw,uname) values (#{uid},#{upw},#{uname})
	</insert>

	<insert id="insertRole">
		insert into tbl_role(rname,uid) values(#{rname},#{uid})
	</insert>


</mapper>